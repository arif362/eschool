<div class="container">
  <h2 class="page-header">Enter User Name and Password</h2>
  <%= notice %>
  <%= alert %>
  <%= form_tag do %>
  <fieldset>
    <legend> Login</legend>
        <div class="form-group">
          <label>User Name:</label>
          <%= text_field_tag :user_name, params[:user_name] ,class:'form-control'%>
        </div>
        <div class="form-group">
          <label>Password:</label>
          <%= text_field_tag :password, params[:password],class:'form-control' %>
        </div>
        <div class="form-group">
          <%= submit_tag 'Login' , class:' btn btn-default'%>
        </div>
  </fieldset>
    <% end %>
  <p>Not Yet Account ?</p><%= link_to 'Sign Up',new_user_path%>
</div>